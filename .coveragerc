# Project Sullivan - Coverage Configuration

[run]
source = src
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */setup.py
    */.venv/*
    */venv/*
    */venv_sullivan/*

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

show_missing = True
skip_covered = False

[html]
directory = htmlcov
